{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5|^8.0",
        "laravel/framework": "^7.29",
        "aws/aws-sdk-php": "^3.172",
        "maatwebsite/excel": "^3.1.8",
        "laravelcollective/html": "^6.2",
        "spatie/laravel-analytics": "^3.10",
        "laravelista/ekko": "~1.2",
        "spatie/laravel-permission": "^4.0",
        "barryvdh/laravel-snappy": "^0.4",
        "khill/lavacharts": "~3.1",
        "laracasts/utilities": "^3.2",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "fideloper/proxy": "^4.4",
        "webpatser/laravel-uuid": "^2.0",
        "goat1000/svggraph": "^3.0",
        "barryvdh/laravel-debugbar": "^3.5",
        "martinlindhe/laravel-vue-i18n-generator": "0.1.46",
        "spatie/laravel-backup": "^6.14",
        "laravel/tinker": "^2.5",
        "daltcore/lara-pdf-merger": "^2.0",
        "mikehaertl/php-pdftk": "^0.8.0",
        "laravel/ui": "^2.5",
        "fruitcake/laravel-cors": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "laracasts/generators": "^2.0",
        "nunomaduro/collision": "^4.3",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
