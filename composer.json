{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.27",
        "aws/aws-sdk-php": "^3.172",
        "maatwebsite/excel": "^3.1",
        "laravelcollective/html": "^6.2",
        "spatie/laravel-analytics": "^3.10",
        "laravelista/ekko": "~1.2",
        "spatie/laravel-permission": "^4.0",
        "barryvdh/laravel-snappy": "^0.4",
        "khill/lavacharts": "~3.1",
        "laracasts/utilities": "^3.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "fideloper/proxy": "^4.4",
        "webpatser/laravel-uuid": "^2.0",
        "goat1000/svggraph": "^3.0",
        "barryvdh/laravel-debugbar": "^3.5",
        "martinlindhe/laravel-vue-i18n-generator": "0.1.46",
        "spatie/laravel-backup": "^6.14",
        "laravel/tinker": "^2.5",
        "daltcore/lara-pdf-merger": "^2.0",
        "mikehaertl/php-pdftk": "^0.8.0",
        "fruitcake/laravel-cors": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3",
        "laracasts/generators": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^2.5",
        "nunomaduro/collision": "^5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
